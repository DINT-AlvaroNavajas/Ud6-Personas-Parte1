<Window x:Class="EjercicioPersonas.Dialogo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EjercicioPersonas"
        mc:Ignorable="d"
        Title="Añadir nacionalidad" Height="125" Width="400"
        ResizeMode="NoResize"
        ShowInTaskbar="False">
    <StackPanel Margin="10">
        <TextBlock Text="Nacionalidad" />
        <TextBox />
        <StackPanel Orientation="Horizontal" Margin="0,10,0,0" HorizontalAlignment="Right">
            <Button Content="Aceptar" Margin="0,0,5,0" Width="65"/>
            <Button Content="Cancelar" Width="65"/>
        </StackPanel>
    </StackPanel>
</Window>
